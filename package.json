{
  "name": "react-emotion-starter",
  "version": "1.3.0",
  "description": "Starter kit to easily create SPA based on react and emotion.",
  "keywords": [
    "boilerplate",
    "react",
    "emotion",
    "typescript",
    "vite",
    "recoil",
    "react-query",
    "i18next"
  ],
  "type": "module",
  "author": "one-chance",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/one-chance/react-emotion-starter.git"
  },
  "bin": {
    "react-emotion-starter": "./bin/react-emotion-starter.js"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@tanstack/react-query": "^5.17.1",
    "i18next": "^23.15.2",
    "i18next-browser-languagedetector": "7.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "14.1.3",
    "react-router-dom": "^6.26.2",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  }
}